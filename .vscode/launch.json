{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "chrome",
			"request": "launch",
			"name": "Debug Frontend (Vite)",
			"url": "http://localhost:5173",
			"webRoot": "${workspaceFolder}/packages/frontend/src"
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Debug Backend (Wrangler)",
			"port": 9229,
			"restart": true,
			"protocol": "inspector",
			"skipFiles": ["<node_internals>/**"]
		}
	]
}
